<div>
	@using (Html.BeginForm()) {
		<div class="form-group row mb-3">
			@Html.LabelFor(model => model.SelectedItem, new { @class = "control-label col-form-label col-md-2" })
			<div class="col-md-4">
				@Html.DropDownListFor(x => x.SelectedItem, new SelectList(Model.Options, "SkinName", "SkinName"), new { @class = "form-control form-select smtext" })
				@Html.ValidationMessageFor(model => model.SelectedItem)
			</div>
			<div col-md-2">
				<input type="submit" value="Apply" class="btn btn-primary" />
			</div>
		</div>
	}
