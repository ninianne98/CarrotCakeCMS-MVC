		<div class="form-group row mb-3">
			<label class="control-label col-form-label col-md-4">Date Sample</label>
			<div class="col-md-3">
				<div class="input-group">
					@Html.TextBox("SampleDate", DateTime.Now, "{0:MM/dd/yyyy}", new { maxlength = "12", @class = "date-picker form-control" })
					<label for="SampleDate" class="input-group-addon input-group-text">
						<span class="glyphicon glyphicon-calendar bi bi-clock-history"></span>
					</label>
				</div>
			</div>
		</div>

		<div class="form-group row mb-3">
			<label class="control-label col-form-label col-md-4">Time Sampler</label>
			<div class="col-md-3">
				<div class="input-group">
					@Html.TextBox("SampleTime", DateTime.Now, "{0:hh:mm tt}", new { maxlength = "16", @class = "time-picker form-control" })
					<label for="SampleTime" class="input-group-addon input-group-text">
						<span class="glyphicon glyphicon-time bi bi-calendar3"></span>
					</label>
				</div>
			</div>
		</div>

		<div class="form-group row mb-3">
			<label class="control-label col-form-label col-md-4">Multi-Select Sampler</label>
			<div class="col-md-5">
				@Html.ListBox("SelSampleChosen",
							 new MultiSelectList(dates, "Key", "Value"),
							new { @class = "form-control chosen-select", @data_placeholder = "Select Dates" })
			</div>
		</div>

		<script type="text/javascript">
			$(document).ready(function () {
				$(".chosen-select").chosen({ width: "98%" });
			});
		</script>
	</div>
</div>

